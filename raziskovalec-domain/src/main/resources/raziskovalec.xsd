<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://www.raziskovalec.org/podatkovnaDomena"
	elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://www.raziskovalec.org/podatkovnaDomena" xmlns:pref="http://www.raziskovalec.org/UserTypes">

	<import schemaLocation="UserTypes.xsd" namespace="http://www.raziskovalec.org/UserTypes"></import>
	<complexType name="Researcher">
		<sequence>
            <element name="id" type="pref:ID"></element>
            <element name="firstName" type="string"></element>
			<element name="lastName" type="string"></element>
			<element name="telephoneNumber"
				type="pref:TelephoneNumber">
			</element>
			<element name="email" type="pref:Email"></element>
			<element name="www" type="pref:WWW" minOccurs="0"
				maxOccurs="1">
			</element>
			<element name="parentOrganization"
				type="tns:Organization">
			</element>
		</sequence>
	</complexType>

	<complexType name="Organization">
		<sequence>
            <element name="id" type="pref:ID"></element>
            <element name="name" type="string"></element>
			<element name="telephoneNumber"
				type="pref:TelephoneNumber">
			</element>
			<element name="email" type="pref:Email"></element>
			<element name="www" type="pref:WWW" minOccurs="0"
				maxOccurs="1">
			</element>
		</sequence>
	</complexType>

</schema>